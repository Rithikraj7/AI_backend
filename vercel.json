{
  "version": 2,
  "builds": [
    {
      "src": "App/main.py",
      "use": "@vercel/python",
      "config": {
        "pythonVersion": "3.8"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "App/main.py"
    }
  ],
  "build": {
    "env": {
      "CUSTOM_BUILD_COMMAND": "pip install -r /requirements.txt"
    }
  }
}
